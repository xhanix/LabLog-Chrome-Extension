{
  "manifest_version": 2,
  "name": "LabLog",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "LabLog"
  },
  "permissions": [
    "https://labnotebook/*",
    "file:///*",
    "http://localhost:8000/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [{
    "all_frames": true,
    "matches": [
      "https://labnotebook.app/*",
      "http://localhost:8000/*"
    ],
    "js": [
      "content.js"
    ]
  }],
  "minimum_chrome_version": "46.0"
  
}